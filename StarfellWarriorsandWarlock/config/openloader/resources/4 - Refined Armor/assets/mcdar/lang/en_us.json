  "enchantment.mcdar.beast_boss.desc":                    "Increases the amount of damage your pets deal.",
  "enchantment.mcdar.beast_burst.desc":                   "Using a healing potion causes an explosion at your pets' locations, dealing damage to the pet and mobs around them relative to enchantment level.",
  "enchantment.mcdar.beast_surge.desc":                   "Using your health potion boosts your pets' attack and movement speed for 10 seconds.",
  "enchantment.mcdar.cooldown.desc":                      "Artifact cooldowns are decreased.",

  "tooltip_info_item.mcdar.blast_fungus_1":               "§o§7Only the bravest of warriors carry",
  "tooltip_info_item.mcdar.blast_fungus_2":               "§o§7the Blast Fungus. Not just because",
  "tooltip_info_item.mcdar.blast_fungus_3":               "§o§7of its toxic spores, but because",
  "tooltip_info_item.mcdar.blast_fungus_4":               "§o§7it smells awful.",

  "tooltip_info_item.mcdar.boots_of_swiftness_1":         "§o§7Boots blessed with enchantments",
  "tooltip_info_item.mcdar.boots_of_swiftness_2":         "§o§7to allow for swift movements. Useful",
  "tooltip_info_item.mcdar.boots_of_swiftness_3":         "§o§7in uncertain times such as these.",

  "tooltip_info_item.mcdar.buzzy_nest_1":                 "§o§7Bee lovers and the bee-loved alike",
  "tooltip_info_item.mcdar.buzzy_nest_2":                 "§o§7are fans of the Buzzy Nest, but don't",
  "tooltip_info_item.mcdar.buzzy_nest_3":                 "§o§7be fooled by the cute bees within -",
  "tooltip_info_item.mcdar.buzzy_nest_4":                 "§o§7they, pack a powerful sting!",

  "tooltip_info_item.mcdar.corrupted_beacon_1":           "§o§7The Corrupted Beacon holds immense",
  "tooltip_info_item.mcdar.corrupted_beacon_2":           "§o§7power within. It waits for the",
  "tooltip_info_item.mcdar.corrupted_beacon_3":           "§o§7moment to unleash its wrath.",

  "tooltip_info_item.mcdar.corrupted_seeds_1":            "§o§7A pouch of poisonous, corrupted seeds",
  "tooltip_info_item.mcdar.corrupted_seeds_2":            "§o§7which grow into spiky grapple vines,",
  "tooltip_info_item.mcdar.corrupted_seeds_3":            "§o§7entangling and slowly draining the",
  "tooltip_info_item.mcdar.corrupted_seeds_4":            "§o§7life from its victims.",

  "tooltip_info_item.mcdar.death_cap_mushroom_1":         "§o§7Eaten by daring warriors before battle,",
  "tooltip_info_item.mcdar.death_cap_mushroom_2":         "§o§7the Death Cap Mushroom drives fighters",
  "tooltip_info_item.mcdar.death_cap_mushroom_3":         "§o§7into a frenzy.",

  "tooltip_info_item.mcdar.enchanted_grass_1":            "§o§7Just as there are powerful heroes who",
  "tooltip_info_item.mcdar.enchanted_grass_2":            "§o§7answer the call to fight, there are",
  "tooltip_info_item.mcdar.enchanted_grass_3":            "§o§7powerful enchanted sheep who will",
  "tooltip_info_item.mcdar.enchanted_grass_4":            "§o§7join the fight when summoned.",

  "tooltip_info_item.mcdar.enchanters_tome_1":            "§o§7Meant only to be wielded by Enchanters,",
  "tooltip_info_item.mcdar.enchanters_tome_2":            "§o§7the magic of this artifact can summon",
  "tooltip_info_item.mcdar.enchanters_tome_3":            "§o§7powerful enchantments.",

  "tooltip_info_item.mcdar.flaming_quiver_1":             "§o§7This quiver is filled with the deadliest",
  "tooltip_info_item.mcdar.flaming_quiver_2":             "§o§7of arrows.",

  "tooltip_info_item.mcdar.ghost_cloak_1":                "§o§7The souls trapped within the Ghost Cloak",
  "tooltip_info_item.mcdar.ghost_cloak_2":                "§o§7are protective, but they radiate a sense",
  "tooltip_info_item.mcdar.ghost_cloak_3":                "§o§7of melancholy.",

  "tooltip_info_item.mcdar.golem_kit_1":                  "§o§7Iron Golems have always protected the",
  "tooltip_info_item.mcdar.golem_kit_2":                  "§o§7Villagers of the Overworld. Their",
  "tooltip_info_item.mcdar.golem_kit_3":                  "§o§7numbers are dwindling as a result",
  "tooltip_info_item.mcdar.golem_kit_4":                  "§o§7of the Arch Illager's war.",

  "tooltip_info_item.mcdar.gong_of_weakening_1":          "§o§7This ancient gong, marked with the",
  "tooltip_info_item.mcdar.gong_of_weakening_2":          "§o§7symbols of a nameless kingdom,",
  "tooltip_info_item.mcdar.gong_of_weakening_3":          "§o§7feels safe in your hands but emits",
  "tooltip_info_item.mcdar.gong_of_weakening_4":          "§o§7a menacing hum to those nearby.",

  "tooltip_info_item.mcdar.harpoon_quiver_1":             "§o§7This quiver holds a number of harpoons",
  "tooltip_info_item.mcdar.harpoon_quiver_2":             "§o§7that swiftly pierce through enemies on",
  "tooltip_info_item.mcdar.harpoon_quiver_3":             "§o§7land or underwater.",

  "tooltip_info_item.mcdar.harvester_1":                  "§o§7The Harvester siphons the souls",
  "tooltip_info_item.mcdar.harvester_2":                  "§o§7of the dead, before releasing",
  "tooltip_info_item.mcdar.harvester_3":                  "§o§7them into a cluster hex of power.",

  "tooltip_info_item.mcdar.iron_hide_amulet_1":           "§o§7The Iron Hide Amulet is both ancient",
  "tooltip_info_item.mcdar.iron_hide_amulet_2":           "§o§7and timeless. Sand mysteriously and",
  "tooltip_info_item.mcdar.iron_hide_amulet_3":           "§o§7endlessly slips through the cracks",
  "tooltip_info_item.mcdar.iron_hide_amulet_4":           "§o§7in the iron.",

  "tooltip_info_item.mcdar.light_feather_1":              "§o§7No one knows what mysterious creature",
  "tooltip_info_item.mcdar.light_feather_2":              "§o§7this feather came from, but it is as",
  "tooltip_info_item.mcdar.light_feather_3":              "§o§7beautiful and powerful.",

  "tooltip_info_item.mcdar.lightning_rod_1":              "§o§7Crafted by Illager Geomancers, this",
  "tooltip_info_item.mcdar.lightning_rod_2":              "§o§7item is enchanted with the power of",
  "tooltip_info_item.mcdar.lightning_rod_3":              "§o§7a storming sky.",

  "tooltip_info_item.mcdar.love_medallion_1":             "§o§7A spell radiates from this trinket,",
  "tooltip_info_item.mcdar.love_medallion_2":             "§o§7enchanting those nearby into a trance",
  "tooltip_info_item.mcdar.love_medallion_3":             "§o§7where they must protect the holder of",
  "tooltip_info_item.mcdar.love_medallion_4":             "§o§7the, medallion at all costs.",

  "tooltip_info_item.mcdar.powershaker_1":                "§o§7The Powershaker is a smashing good",
  "tooltip_info_item.mcdar.powershaker_2":                "§o§7time, though it may not be as fun",
  "tooltip_info_item.mcdar.powershaker_3":                "§o§7for your enemies.",

  "tooltip_info_item.mcdar.satchel_of_elements_1":        "§o§7Mysteries surround this primordial",
  "tooltip_info_item.mcdar.satchel_of_elements_2":        "§o§7satchel. Will it unleash fire, ice,",
  "tooltip_info_item.mcdar.satchel_of_elements_3":        "§o§7or something a lot less nice?",

  "tooltip_info_item.mcdar.satchel_of_elixirs_1":         "§o§7The Satchel of Elixirs always contains",
  "tooltip_info_item.mcdar.satchel_of_elixirs_2":         "§o§7the exact potions you need! (Well, at",
  "tooltip_info_item.mcdar.satchel_of_elixirs_3":         "§o§7least the potions it thinks you need,",
  "tooltip_info_item.mcdar.satchel_of_elixirs_4":         "§o§7which is still pretty helpful.)",

  "tooltip_info_item.mcdar.satchel_of_snacks_1":          "§o§7The Satchel of Snacks provides a treat",
  "tooltip_info_item.mcdar.satchel_of_snacks_2":          "§o§7when you need it the most!",

  "tooltip_info_item.mcdar.shock_powder_1":               "§o§7Shock Powder is a reliable tool for",
  "tooltip_info_item.mcdar.shock_powder_2":               "§o§7those who wish to make a swift exit.",

  "tooltip_info_item.mcdar.soul_healer_1":                "§o§7The Soul Healer amulet is cold to",
  "tooltip_info_item.mcdar.soul_healer_2":                "§o§7the touch and trembles with the power",
  "tooltip_info_item.mcdar.soul_healer_3":                "§o§7of souls. It is a common among the",
  "tooltip_info_item.mcdar.soul_healer_4":                "§o§7Illagers of the Woodland Mansions.",

  "tooltip_info_item.mcdar.tasty_bone_1":                 "§o§7You hear distant howling as you hold",
  "tooltip_info_item.mcdar.tasty_bone_2":                 "§o§7the Tasty Bone in your hand.",

  "tooltip_info_item.mcdar.thundering_quiver_1":          "§o§7This copper quiver channels",
  "tooltip_info_item.mcdar.thundering_quiver_2":          "§o§7the power of lightning.",

  "tooltip_info_item.mcdar.torment_quiver_1":             "§o§7The Torment Quiver radiates powerful",
  "tooltip_info_item.mcdar.torment_quiver_2":             "§o§7energy drawn from the eternal source",
  "tooltip_info_item.mcdar.torment_quiver_3":             "§o§7of the Undead.",

  "tooltip_info_item.mcdar.totem_of_regeneration_1":      "§o§7This hand-crafted wooden figurine radiates",
  "tooltip_info_item.mcdar.totem_of_regeneration_2":      "§o§7a warmth like that of a crackling campfire,",
  "tooltip_info_item.mcdar.totem_of_regeneration_3":      "§o§7healing those who gather around it.",

  "tooltip_info_item.mcdar.totem_of_shielding_1":         "§o§7This totem has mystical powers that shield",
  "tooltip_info_item.mcdar.totem_of_shielding_2":         "§o§7those around it from projectiles.",

  "tooltip_info_item.mcdar.totem_of_soul_protection_1":   "§o§7This improvement on the Totem of",
  "tooltip_info_item.mcdar.totem_of_soul_protection_2":   "§o§7Undying, crafted by Master Evokers,",
  "tooltip_info_item.mcdar.totem_of_soul_protection_3":   "§o§7can protect more than just the holder.",

  "tooltip_info_item.mcdar.updraft_tome_1":               "§o§7An ancient book filled with illegible",
  "tooltip_info_item.mcdar.updraft_tome_2":               "§o§7glyphs, you feel a strange breeze as",
  "tooltip_info_item.mcdar.updraft_tome_3":               "§o§7you flip the pages.",

  "tooltip_info_item.mcdar.wind_horn_1":                  "§o§7When the Wind Horn echoes throughout",
  "tooltip_info_item.mcdar.wind_horn_2":                  "§o§7the forests of the Overworld the creatures",
  "tooltip_info_item.mcdar.wind_horn_3":                  "§o§7of the night tremble with fear.",

  "tooltip_info_item.mcdar.wonderful_wheat_1":            "§o§7This item has the faint smell of an",
  "tooltip_info_item.mcdar.wonderful_wheat_2":            "§o§7open field, baked bread, and freshly",
  "tooltip_info_item.mcdar.wonderful_wheat_3":            "§o§7cut crops."
}
